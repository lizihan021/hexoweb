<ul class="Index">

  <% site.categories.findOne({name: 'blogs'}).posts.sort('date', -1).each(function (post) { %>
    <li>
      <%- partial('_partial/list-view', { post: post }) %>
    </li>
  <% }) %>
</ul>
<% if (page.total > 1){ %>
  <section id="nav-wrapper">
    <nav id="page-nav">
      <%- paginator({
        prev_text: "« prev",
        next_text: "next »"
      }) %>
    </nav>
  </section>
<% } %>
